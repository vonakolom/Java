import java.util.Scanner;
public class Loop{
    public static void main(String[] args){
        int i;
        Scanner input = new Scanner(System.in);
        System.out.print("Введите точку отсчёта чётных чисел до 100: ");
        i = input.nextInt();
        while(i <= 100){
            if(i % 2 == 0)
                System.out.println(i);
            i++;
        }
    }
}
